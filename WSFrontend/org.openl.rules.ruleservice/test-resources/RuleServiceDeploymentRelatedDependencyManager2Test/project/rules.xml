<project>
  <name>Lazy-fails</name>
  <modules>
    <module>
      <name>Datatypes</name>
      <rules-root path="Datatypes.xlsx"/>
    </module>
    <module>
      <name>Algorithm</name>
      <rules-root path="Algorithm.xlsx"/>
      <method-filter>
        <includes>
          <value>.+ calc\(.+\)</value>
        </includes>
      </method-filter>
    </module>
  </modules>
</project>